{
  "extends": "airbnb",
  "rules": {
    "react/prefer-es6-class": 1,
    "react/jsx-filename-extension": 0,
    "react/forbid-prop-types": 1,
    "react/prefer-stateless-function": 0,
    "react/jsx-no-bind": 1,
    "jsx-a11y/no-static-element-interactions": 1,
    "linebreak-style": 0,
  },
  "globals": {
    "document": 1,
    "window": 1,
    "fetch": 1,
    "sessionStorage": 1,
    "localStorage": 1,
  }
}
